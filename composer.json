{
  "name": "bronos/php-sql-migrations",
  "description": "PHP SQL templates",
  "keywords": [
    "php",
    "db",
    "sql",
    "database",
    "templates"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Oleg Bronzov",
      "email": "oleg.bronzov@gmail.com"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:BronOS/php-sql-schema.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:BronOS/php-sql-discovery.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:BronOS/php-sql-diff.git"
    }
  ],
  "require": {
    "php": ">=7.4",
    "ext-pdo": "*",
    "bronos/php-enum": "*",
    "bronos/php-sql-schema": "dev-master",
    "bronos/php-sql-diff": "dev-master",
    "bronos/php-sql-discovery": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "*"
  },
  "autoload": {
    "psr-4": {
      "BronOS\\PhpSqlMigrations\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BronOS\\PhpSqlMigrations\\Tests\\": "tests/"
    },
    "scripts": {
      "test": "vendor/bin/phpunit --coverage-clover=coverage.xml"
    }
  }
}
